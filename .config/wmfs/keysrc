        # change screen
        [key] mod = {"Super"} key = "Tab" func = "screen_next" [/key]	
        [key] mod = {"Siuper", "Shift"} key = "Tab" func = "screen_prev" [/key]

        # swap client in the next/prev screen
        [key] mod = {"Super", "Shift"} key = "a" func = "client_screen_next" [/key]
        [key] mod = {"Control", "Shift"} key = "z" func = "client_screen_prev" [/key]

        # Toggle tag explose
        [key] mod = {"Mod4"} key = "e" func = "toggle_tag_expose" [/key]

        # Toggle split mode
        [key] mod = {"Mod4"} key = "s" func = "split_toggle" [/key]

        # Focus next client with direction
        [key] mod = {"Mod4"} key = "h" func = "client_focus_left"   [/key]
        [key] mod = {"Mod4"} key = "l" func = "client_focus_right"  [/key]
        [key] mod = {"Mod4"} key = "k" func = "client_focus_top"    [/key]
        [key] mod = {"Mod4"} key = "j" func = "client_focus_bottom" [/key]

        # Swap next client with direction
        [key] mod = {"Control"} key = "h" func = "client_swap_left"   [/key]
        [key] mod = {"Control"} key = "l" func = "client_swap_right"  [/key]
        [key] mod = {"Control"} key = "k" func = "client_swap_top"    [/key]
        [key] mod = {"Control"} key = "j" func = "client_swap_bottom" [/key]

        # Move next splitted client with direction
        [key] mod = {"Control", "Shift"} key = "h" func = "split_move_left"   [/key]
        [key] mod = {"Control", "Shift"} key = "l" func = "split_move_right"  [/key]
        [key] mod = {"Control", "Shift"} key = "k" func = "split_move_top"    [/key]
        [key] mod = {"Control", "Shift"} key = "j" func = "split_move_bottom" [/key]

        # Resize selected tiled client with direction
        [key] mod = {"Mod4","Control"} key = "h" func = "client_resize_left"   cmd = "20" [/key]
        [key] mod = {"Mod4","Control"} key = "l" func = "client_resize_right"  cmd = "20" [/key]
        [key] mod = {"Mod4","Control"} key = "k" func = "client_resize_top"    cmd = "20" [/key]
        [key] mod = {"Mod4","Control"} key = "j" func = "client_resize_bottom" cmd = "20" [/key]
        [key] mod = { "Alt", "Control"} key = "h" func = "client_resize_right"  cmd = "-20" [/key]
        [key] mod = { "Alt", "Control"} key = "l" func = "client_resize_left"   cmd = "-20" [/key]
